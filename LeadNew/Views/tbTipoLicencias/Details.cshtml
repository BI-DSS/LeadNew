@model LeadNew.Models.tbTipoLicencia

@{
    ViewData["Title"] = "Details";
}

    <div style="padding-top: 5em;">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "tbTipoLicencias")">Listado de licencias</a></li>
                <li class="breadcrumb-item active" aria-current="page">detalles licencia</li>
            </ol>
        </nav>
        <h2>Detalles</h2>
        <div>
            <dl class="dl-horizontal">
                <dt>
                    Descripción
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.tlicDescripcion)
                </dd>
                <dt>
                    Creado por
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.tlicUsuarioCrea)
                </dd>
                <dt>
                    Fecha de creación
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.tlicFechaCrea)
                </dd>
                <dt>
                    Modificado por
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.tlicUsuarioModifica)
                </dd>
                <dt>
                    Fecha de modificación
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.tlicFechaModifica)
                </dd>
            </dl>
        </div>
    </div>





