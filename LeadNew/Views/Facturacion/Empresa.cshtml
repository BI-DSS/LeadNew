
@foreach (var item in ViewData["Empresa"] as IEnumerable<LeadNew.Models.tbEmpresa>)
{
    <div class="col-sm-6">
        <div class="form-group">
            <label for="formGroupExampleInput">Telefóno:</label>
            <input type="text" class="form-control" id="txt_telefono" disabled value="@item.empTelefono">
        </div>
    </div>

    <div class="col-sm-12">
        <div class="form-group">
            <label for="formGroupExampleInput">Dirección:</label>
            <textarea class="form-control" disabled>@item.empDireccion</textarea>
        </div>
    </div>
}
