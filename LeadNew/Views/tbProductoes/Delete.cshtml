@model LeadNew.Models.tbProducto

@{
    ViewData["Title"] = "Eliminar " + " ";
}

    <div style="padding-top: 5em;">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "tbProductoes")">Listado de productos</a></li>
                <li class="breadcrumb-item active" aria-current="page">eliminar productos</li>
            </ol>
        </nav>
        <h2>Eliminar</h2>
        <form asp-action="Delete">
            <h3>¿Estás seguro de eliminar el siguiente producto @Model.prDetalle?</h3>
            <input type="hidden" asp-for="prId" />
            <input type="submit" value="Eliminar" class="btn btn-danger" />
        </form>
    </div>
