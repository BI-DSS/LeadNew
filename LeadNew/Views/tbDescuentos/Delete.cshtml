@model LeadNew.Models.tbDescuentos

@{
    ViewData["Title"] = "Delete " + " ";
}

<div style="padding-top: 5em;">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
            <li class="breadcrumb-item"><a href="@Url.Action("Index", "tbDescuentos")">Listado de Descuentos</a></li>
            <li class="breadcrumb-item active" aria-current="page">Eliminar</li>
        </ol>
    </nav>
    <h2>Eliminar</h2>
    <form asp-action="Delete">
        <h3>¿Está seguro de eliminar el siguiente descuento @Model.desNombreCampaña?</h3>
        <input type="hidden" asp-for="desId" />
        <input type="submit" value="Eliminar" class="btn btn-danger" />
    </form>
</div>

