@model LeadNew.Models.tbMoneda

@{
    ViewData["Title"] = "Details";
}

    <div style="padding-top: 5em;">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "tbMonedas")">Listado de Monedas</a></li>
                <li class="breadcrumb-item active" aria-current="page">Detalles</li>
            </ol>
        </nav>
        <h2>Detalles</h2>

        <div>
            <dl class="dl-horizontal">
                <dt>
                    Abreviatura:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moAbreviatura)
                </dd>
                <dt>
                    Nombre:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moNombre)
                </dd>
                <dt>
                    Creado por:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moUsuarioCrea)
                </dd>
                <dt>
                    Fecha de modificación
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moFechaCrea)
                </dd>
                <dt>
                    Modificado por:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moUsuarioModifica)
                </dd>
                <dt>
                    Fecha de modificación
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.moFechaModifica)
                </dd>
            </dl>
        </div>
    </div>