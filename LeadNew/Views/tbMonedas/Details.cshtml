@*@model LeadNew.Models.tbMoneda*@

@{
    ViewData["Title"] = "Detalles" + " ";
}

    <div style="padding-top: 5em;">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "tbMonedas")">Listado de Monedas</a></li>
                <li class="breadcrumb-item active" aria-current="page">Detalles</li>
            </ol>
        </nav>
        <h2>Detalles</h2>

        <div>
            @foreach (var item in ViewData["monedas"] as IEnumerable<LeadNew.Models.MonedaVista>)
            {
                <dl class="dl-horizontal">
                    <dt>
                        Abreviatura:
                    </dt>
                    <dd>
                        @item.moAbreviatura
                    </dd>
                    <dt>
                        Nombre:
                    </dt>
                    <dd>
                        @item.moNombre
                    </dd>
                    <dt>
                        Creado por:
                    </dt>
                    <dd>
                        @item.usuNombres @item.usuApellidos
                    </dd>
                    <dt>
                        Fecha de modificación
                    </dt>
                    <dd>
                        @item.moFechaCrea
                    </dd>
                    <dt>
                        Modificado por:
                    </dt>
                    <dd>
                        @item.usuNombresMod @item.usuApellidosMod
                    </dd>
                    <dt>
                        Fecha de modificación
                    </dt>
                    <dd>
                        @item.moFechaModifica
                    </dd>
                </dl>
            }
        </div>
    </div>