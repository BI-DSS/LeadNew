
@{
    ViewData["Title"] = "RegistroTributario";
}

<div style="padding-top: 5em;">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
            <li class="breadcrumb-item active" aria-current="page">Registros Tributarios</li>
        </ol>
    </nav>
    <div class="row">
        @foreach (var item in ViewData["tributario"] as IEnumerable<LeadNew.Models.RegistroTributarioNotificaciones>)
        {
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail" style="height: 12em;">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="caption">
                                <dl class="dl-horizontal">
                                    <dt>
                                        Nombre de la Empresa
                                    </dt>
                                    <dd>
                                        @item.empNombre
                                    </dd>
                                    <dt>
                                        CAI
                                    </dt>
                                    <dd>
                                        @item.rtCAI
                                    </dd>
                                    <dt>
                                        Fecha Limite
                                    </dt>
                                    <dd>
                                        @item.rtFechafinal
                                    </dd>
                                    <dt>
                                        Rango Inicial
                                    </dt>
                                    <dd>
                                        @item.rtRangoAutoInicio
                                    </dd>
                                    <dt>
                                        Rango Final
                                    </dt>
                                    <dd>
                                        @item.rtRangoAutoFinal
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>