@model IEnumerable<LeadNew.Models.tbRegistroTributario>

@{
     ViewData["Title"] = "Index";
}
    <div style="padding-top: 5em;">
        <nav aria-label="breadcrum">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">Resgistro Tributario Activo</li>
            </ol>
        </nav>
        <h2>Resgistros Tributarios</h2>
        <p>
            <a class="btn btn-success" asp-action="Create"><img src="~/images/add-24px.svg" class="img-fluid" alt="Responsive image">Crear Resgistro</a>
        </p>
        <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th>
                        Empresa
                    </th>
                    <th>
                        RTN
                    </th>
                    <th>
                        CAI
                    </th>
                    <th>
                        Fecha de Creacion
                    </th>
                    <th>
                        Fecha de Activacion
                    </th>
                    <th>
                        Fecha Finalizacion
                    </th>
                    <th>
                        Rango Autorizado Inicio
                    </th>
                    <th>
                        Rango Autorizado Final
                    </th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtIdEmpresa)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtRTN)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtCAI)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtFechaCreacion)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtFechainicio)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtFechafinal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtRangoAutoInicio)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.rtRangoAutoFinal)
                    </td>
                    <td>
                        <a class="btn btn-primary btn-sm" asp-action="Edit" asp-route-id="@item.rtId"><img src="~/images/create-24px.svg" class="img-fluid" alt="Responsive image">Editar</a>
                        <a class="btn btn-info btn-sm" asp-action="Details" asp-route-id="@item.rtId"><img src="~/images/remove_red_eye-24px.svg" class="img-fluid" alt="Responsive image">Detalles</a>
                        <a class="btn btn-danger btn-sm" asp-action="Delete" asp-route-id="@item.rtId"><img src="~/images/delete-24px.svg" class="img-fluid" alt="Responsive image">Eliminar</a>
                    </td>
                </tr>
                }
            </tbody>
        </table>


    </div>